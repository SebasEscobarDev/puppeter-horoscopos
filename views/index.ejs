<body>
	<div class="container">
		<div class="row">
			<% var count = 0
			const allPredictions = JSON.parse(predicciones)
			allPredictions.forEach((pre, i) => { %>
				<%count++%>
				<div class="col-md-6">
				
		      <div id="editor<%- i %>"><h1># <%-count%> <%-pre.signo%></h1></div>
		      <div id="editor<%- i %>"><%-pre.prediccion%></div>
		      <br>
		      <br>
		      <br>
		    </div>
			<% }) %>
		</div>
	</div>
	
	<footer>

	</footer>

